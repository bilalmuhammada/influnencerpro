{
    "compilerOptions": {
      "rootDir": "src",
      "outDir": "dist",
      "target": "es3",
      "module": "es6",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "allowSyntheticDefaultImports": true,
      "removeComments": false,
      "preserveConstEnums": true,
      "sourceMap": true
    }
  }
